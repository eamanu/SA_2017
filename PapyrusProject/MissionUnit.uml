  <Requirements:Requirement xmi:id="_u-fEkP2zEeWFwtaYqXT6rw" text="The Mission Unit shall be designed for acquiring data for a period of time not less than six months." id="L2B-SYS-720" base_Class="_u-edgP2zEeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_NLfOsP20EeWFwtaYqXT6rw" text="The Mission Unit shall be designed for archiving and publishing data for a period of time not less than six months once the spaacecraft is decommissioned." id="L2B-SYS-721" base_Class="_NLenoP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_Ni96kP20EeWFwtaYqXT6rw" text="The Mission Unit shall provide a CAL/VAL platform to handle calibration-related data." id="L2B-SYS-722" base_Class="_Ni9TgP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_OA07Qf20EeWFwtaYqXT6rw" text="The Mission Unit shall provide a CAL/VAL platform to allow users to download CAL/VAL data and submit CAL/VAL requests." id="L2B-SYS-723" base_Class="_OA07QP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_OjI-If20EeWFwtaYqXT6rw" text="The Mission Unit shall provide a platform for user access to products." id="L2B-SYS-724" base_Class="_OjI-IP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_O4cfwP20EeWFwtaYqXT6rw" text="The Mission Unit shall provide a platform for user access to telemetry, telecommands and housekepping." id="L2B-SYS-725" base_Class="_O4b4sP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_PM0NQP20EeWFwtaYqXT6rw" text="The Mission Unit shall define role-based access control for users." id="L2B-SYS-726" base_Class="_PMzmMP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_PgxEAP20EeWFwtaYqXT6rw" text="The Mission Unit shall process science data up to L1B / L2 products, according to the definitions set forth by the Committe on Earth Observation Satellites (CEOS)(TBC)." id="L2B-SYS-727" base_Class="_Pgwc8P20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_P4F-4P20EeWFwtaYqXT6rw" text="The Mission Unit shall store raw data." id="L2B-SYS-728" base_Class="_P4FX0P20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_Rfid0f20EeWFwtaYqXT6rw" text="The Mission Unit shall store products (TBC)." id="L2B-SYS-729" base_Class="_Rfid0P20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_R12FAf20EeWFwtaYqXT6rw" text="The Mission Unit shall store telemetry, telecommands and housekepping." id="L2B-SYS-730" base_Class="_R12FAP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_SgFU0P20EeWFwtaYqXT6rw" text="The Mission Unit shall supply acquisition planning." id="L2B-SYS-731" base_Class="_SgEtwP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_Y8msUP20EeWFwtaYqXT6rw" text="The Mission Unit shall implement an interfase with Control Unit." id="L2B-SYS-732" base_Class="_Y8mFQP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_aFaHcf20EeWFwtaYqXT6rw" text="The Mission Unit shall download science data via the SA2017 station." id="L2B-SYS-733" base_Class="_aFaHcP20EeWFwtaYqXT6rw"/>
  <Requirements:Requirement xmi:id="_crVn4f20EeWFwtaYqXT6rw" text="The Mission Unit shall bring SA2017 community access through a web platform on a regular basis." id="L2B-SYS-734" base_Class="_crVn4P20EeWFwtaYqXT6rw"/>
</xmi:XMI>
